class X {
	method get_self_id() {
		self.id
	}
	method get_self_clos() {
		closure () { self.id }
	}
};

var d = X()

print("GET_SELF_ID: " + d.get_self_id().string)
print("GET_SELF_ID2: " + d.get_self_id().string)
print("GET_SELF_CLOS: " + d.get_self_clos()().string)
print("GET_SELF_CLOS2: " + d.get_self_clos()().string)
